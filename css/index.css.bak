@charset "UTF-8";
@media only screen and (min-width:1224px){
	#wrap{
		width:100%;
		background-image:url(../img/backone.png);
		background-color:#000;
	}
	#header{
		width:900px;
		margin:0 auto;
		padding-top:30px;
	}
	#nav_menu{
		
	}
	#footer{
		
		width:900px;
		height:100px;
		margin:10px auto;
		
	}
	#main{
		width:100%;
	}
	#main_content{
		width:900px;
		margin:0 auto;
	}
	#story_content{
		width:900px;
		margin:0 auto;
	}
	#history_content{
		width:900px;
		margin:0 auto;
	}
	#board_content{
		width:900px;
		margin:0 auto;
	}
	.content{
		width:900px;
		margin:0 auto;
		background-color: #f5f5f5;
		border-radius: 4px 4px 4px 4px;
	}
	.accordion{
		width:800px;
		margin:0 auto;
	}
	.carousel-indicators {
		list-style: none outside none;
		margin: 0;
		position: absolute;
		right: 15px;
		top: 15px;
		z-index: 5;
	}
	#my_info{
		float:left;
		width:250px;
		height: 400px;

	}
	#map_canvas{
		float:left;
		width:400px !important;
		height:250px !important;
		border-radius:5px;
		-moz-border-radius:5px;
		-webkit-border-radius:5px;
	}
	.line_clear{
		clear:both;
	}
}

